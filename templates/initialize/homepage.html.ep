		dataType:	'text',
		method:		'POST',
		url:		'/GetTable/design_flower', // See routes at end of WWW::Garden::Design.startup().
	})
	.done(function(image_path)
	{
		if (image_path.length > 0)
		{
			$('#design_flower_thumbnail')
			.html("<img src = '" + image_path + "' " + get_design_image_size_string() + '>');
		}
	});


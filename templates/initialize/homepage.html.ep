function build_attribute_id(kind, name, value)
{
	// We must allow for:
	// o Names like 'Sun tolerance'.
	// o Range values like 'Full sun' and 'Semi-dwarf'.

	name	= name.replace(/\s|-/g, '_');
	value	= value.replace(/\s|-/g, '_');

	return '#' + kind + '_' + name + '_' + value;

} // End of build_attribute_id.

// -----------------------------------------------

function build_full_property_menu(property_id, jquery_id)
{
	var html	= "<select id = '" + jquery_id + "'>";
	var index	= -1;

	var id;
	var name;

	for (var i = 0; i < full_property_table.length; i++)
	{
		id		= full_property_table[i]['id'];
		name	= full_property_table[i]['name'];

		if (id == property_id)
		{
			html	+= "<option value = '" + id + "' selected>" + name + '</option>';
			index	= i;
		}
		else
		{
			html += "<option value = '" + id + "'>" + name + '</option>';
		}
	}

	// With a delete, property_id is 0, so index remains as -1.
	// In this case I set it to the index of the 1st id in the table.

	if (index < 0)
	{
		index = 0;
	}

	html	+= '</select>';
	id		= '#' + jquery_id;

	$(id + '_span')
	.empty()
	.append(html);

	if (jquery_id == 'full_property_menu')
	{
		$(id)
		.selectmenu
		({
			change: function(event, data)
			{
				full_property_menu_changed(data);
			}
		});
	}
	else
	{
		$(id)
		.selectmenu
		({
			change: function(event, data)
			{
				garden_property_menu_2_changed(data);
			}
		});
	}

	return index;

} // End of build_full_property_menu.

// -----------------------------------------------

function build_garden_menu(property_id, garden_id, jquery_id)
{
	var html	= "<select id = '" + jquery_id + "'>";
	var index	= -1;

	var id;
	var name;

	for (var i = 0; i < garden_table.length; i++)
	{
		if (garden_table[i]['property_id'] == property_id)
		{
			id		= garden_table[i]['id'];
			name	= garden_table[i]['name'];

			if (id == garden_id)
			{
				html	+= "<option value = '" + id + "' selected>" + name + '</option>';
				index	= i;
			}
			else
			{
				html += "<option value = '" + id + "'>" + name + '</option>';
			}
		}
	}

	// With a delete, garden_id is 0, so index remains as -1.
	// In this case I set it to the index of the 1st id in the table.

	if (index < 0)
	{
		index = 0;
	}

	html	+= '</select>';
	id		= '#' + jquery_id;

	$(id + '_span')
	.empty()
	.append(html);

	// Warning: These 2 calls to selectmenu() are mandatory.

	if (jquery_id == 'garden_garden_menu')
	{
		$(id)
		.selectmenu
		({
			change: function(event, data)
			{
				garden_garden_menu_changed(data);
			}
		});
	}
	else
	{
		$(id)
		.selectmenu();
	}

	return index;

} // End of build_garden_menu.


function build_image_ids(sequence)
{
	// Names are in alphabetical order.

	var ids =
	[
		'#image_description_' + sequence,	// 0.
		'#image_file_name_' + sequence,		// 1.
		'#image_url_' + sequence,			// 2.
		'#image_sequence_' + sequence		// 3. Not used.
	];

	return ids;

} // End of build_image_ids.


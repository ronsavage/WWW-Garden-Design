// Note: Changed means the user changed the selected item.

function design_property_menu_changed(data)
{
	//	for( key in data )
	//	{
	//		alert( "key is " + [ key ] + ", value is " + data[ key ] );
	//	}

	// Typical keys for data.item are:
	//	o label => The property item's text (name column value).
	//	o value => The property item's # (primary key column value).

	design_current_property_id	= data.item.value;
	var garden_id		= 0;

	for (var i = 0; i < garden_table.length; i++)
	{
		if (garden_table[i]['property_id'] == design_current_property_id)
		{
			design_current_garden_id	= garden_table[i]['id'];
			dummy						= build_garden_menu(design_current_property_id, design_current_garden_id, 'design_garden_menu');

			// No need to call set_garden_fields(index) because the fields are not used  on the Design tab.

			break;
		}
	}

} // End of design_property_menu_changed.

